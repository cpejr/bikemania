<nav class="navbar navbar-light bg-light" id="logobike"></nav>
<main class="full-screen">
  <article class="justify-content-center mx-0">
    <div class="bg-CPE-white Address-btn1">
      <button class="btn btn-default" onclick="location.href='/home'"> <i class="fa fa-shield fa-rotate-90 fa-3x" aria-hidden="true"></i> </button>
      <div class="acomp-header text-center">
        <h3 class="text-CPE-orange">Acompanhamento de Alugueis</h3>
        <h3 class="text-CPE-orange">{{unidade}}</h3>
      </div>
    </div>
  </article>
  <article class="mt-3">
    <section class="row col-12 d-block d-sm-flex justify-content-between mx-auto">
      <div class="text-center">
          <button type="button" id="yellow-beach-btn" onclick="location.href='/signup'" class="btn orange-button btn-lg mt-4">Cadastro</button>
          <button type="button" id="yellow-beach-btn" onclick="location.href='/newRent'" class="btn orange-button btn-lg mt-4">Venda</button>
      </div>
      <div class="text-center">
        <button onclick="location.href = '/logout'" class="btn orange-button btn-lg mt-4">Deslogar</button>
      </div>
    </section>
    <br>
    <section class="container">
      <table class="table table-responsive-sm">
        <thead class="thead-CPE-white">
          <tr>
            <th class="text-center">Nome</th>
            <th class="text-center">CPF</th>
            <th class="text-center">Opções</th>
          </tr>
        </thead>
        <tbody id="table">
          {{#each rents}}
          <tr>
            <td scope="row" class="text-center align-middle font-weight-bold">{{client.name}}</td>
            <td scope="row" class="text-center align-middle font-weight-bold">{{cpf}}</td>
            <td scope="row" class="text-center">
              <form action="/dashboardClient/{{cpf}}" method="get">
              <button class="btn text-white orange-button" type="submit">Ver</button>
            </form>
            </td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </section>
  </article>
</main>
<script src="/javascripts/jquery.js" charset="utf-8"></script>
<script src="/javascripts/moment-with-locales.min.js"></script>
<script src="/javascripts/search-table.js"></script>
<script src="/javascripts/index.js"></script>
