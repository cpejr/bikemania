<button class="btn btn-default" onclick="location.href='/homemaster'"> <i class="fa fa-shield fa-rotate-90 fa-3x"
    aria-hidden="true"></i> </button>

<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">{{mm}}/{{yyyy}}</th>
      <th scope="col">Quantidade</th>
      <th scope="col">Tempo</th>
      <th scope="col">Valor Final</th>
      <th scope="col">Cart√£o</th>
      <th scope="col">Dinheiro</th>

    </tr>
  </thead>
  <tbody>
    {{#each result}}
    <tr>
      <th scope="row">{{day}}</th>
      <td>{{quantidade}}</td>
      <td>{{tempo}}</td>
      <td>{{preco}}</td>
      <td>{{cartao}}</td>
      <td>{{dinheiro}}</td>
    </tr>
    {{/each}}
    <tr>
      <th scope="row">Total</th>
      <td>{{quanttt}}</td>
      <td>{{tempottm}}</td>
      <td>{{precottm}}</td>
      <td>{{cartt}}</td>
      <td>{{dimtt}}</td>
    </tr>
  </tbody>
</table>